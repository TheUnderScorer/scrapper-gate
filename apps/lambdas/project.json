{
  "root": "apps/lambdas",
  "sourceRoot": "apps/lambdas/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/node:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/lambdas",
        "main": "apps/lambdas/src/main.ts",
        "tsConfig": "apps/lambdas/tsconfig.app.json",
        "assets": ["apps/lambdas/src/assets"]
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "apps/lambdas/src/environments/environment.ts",
              "with": "apps/lambdas/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "parallel": false,
        "command": "npx serverless offline --stage development --lambdaPort 3003"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/lambdas/**/*.ts"]
      },
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/lambdas"],
      "options": {
        "jestConfig": "apps/lambdas/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
