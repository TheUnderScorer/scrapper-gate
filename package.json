  "license": "MIT",
  "scripts": {
    "nx": "nx",
    "postnx": "node tools/removeFileExtensionFromTsPaths.ts",
    "start": "nx serve",
    "build": "nx build --prod",
    "build:docker-base": "docker build -f ./docker/base/Dockerfile -t schemik/scrapper-gate-base:latest .",
    "remove-file-extensions-from-ts-paths": "node tools/removeFileExtensionFromTsPaths.ts",
    "build:tools": "cd tools && tsc -p tsconfig.tools.json",
    "watch:tools": "npm run build:tools -- -w",
    "test": "npm run ts-node apps/browser-extension-e2e/src/setup.ts",
    "lint": "nx run-many --target=lint --all --parallel --maxParallel=4",
    "e2e": "nx e2e",
    "serve": "nx run-many --target=serve --parallel --projects=api,browser-extension --maxParallel=2",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "gql-codegen": "graphql-codegen --config codegen.yml",
    "ts-node-dev": "TS_NODE_PROJECT=tsconfig.node.json ts-node-dev -r tsconfig-paths/register",
    "send-sqs-message": "TS_NODE_PROJECT=tsconfig.node.json ts-node-dev -r tsconfig-paths/register -r dotenv/config tools/cli/sendScrapperRunnerMessage.ts dotenv_config_path=\".local.env\" --",
    "ts-node": "TS_NODE_PROJECT=tsconfig.node.json ts-node -r tsconfig-paths/register --",
    "gather-projects": "npm run ts-node tools/scripts/gatherProjects.ts --",
    "release": "npx semantic-release",
    "release:dry": "npx semantic-release -d",
    "typecheck": "tsc --noEmit -p tsconfig.base.json",
    "start-local-env": "node scripts/startLocalEnv.mjs",
    "stop-local-env": "cd deploy && pulumi stack select development && pulumi destroy -y && cd .. && docker compose down -v"
  },
