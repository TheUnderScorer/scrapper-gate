    "browser-extension": {
      "root": "apps/browser-extension",
      "sourceRoot": "apps/browser-extension/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "maxWorkers": 2,
            "progress": true,
            "outputPath": "dist/apps/browser-extension",
            "index": "apps/browser-extension/src/index.html",
            "main": "apps/browser-extension/src/main.tsx",
            "polyfills": "apps/browser-extension/src/polyfills.ts",
            "tsConfig": "apps/browser-extension/tsconfig.app.json",
            "assets": [
              "apps/browser-extension/src/favicon.ico",
              "apps/browser-extension/src/assets"
            ],
            "styles": ["apps/browser-extension/src/styles.css"],
            "scripts": [],
            "webpackConfig": "apps/browser-extension/webpack.config.js",
            "vendorChunk": false
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/browser-extension/src/environments/environment.ts",
                  "with": "apps/browser-extension/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "outputs": ["{options.outputPath}"],
          "options": {
            "buildTarget": "browser-extension:build",
            "maxWorkers": 2
          },
          "configurations": {
            "production": {
              "buildTarget": "browser-extension:build:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/browser-extension/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/browser-extension"],
          "options": {
            "jestConfig": "apps/browser-extension/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
